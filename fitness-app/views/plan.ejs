<!DOCTYPE html>
<html>
<head>
    <title>Your Plan</title>
    <link rel="stylesheet" href="/style.css"></head>
<body>
    <h1>Fit Buddy is here to help you get your fit body!</h1>
    <h2>Your Workout and Meal Plan</h2>
    <h3>Workout Plan</h3>
    <ul>
        <% workoutPlan.forEach(workout => { %>
            <li><%= workout.name %> - <%= workout.type %></li>
        <% }) %>
    </ul>
    <h3>Meal Plan</h3>
    <ul>
        <% mealPlan.forEach(meal => { %>
            <li><%= meal.title %> - <%= meal.calories %> calories</li>
        <% }) %>
    </ul>
</body>
</html>